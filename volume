#include <iostream>
using namespace std;
int main()
{
    char op;
    float M_PI*r*r*h;
    cout <<"Enter oparand: +,-,*,/";
    
    cin >> op;

    cout <<"Volume of a cylinder:";
    cin >> M_PI >> >> r >> r >> h;
    switch(op) {

        case '*':
        cout << M_PI << "*" << r << "*" << r << "*" << h;
        << "="<< 3.141592 * r *r * h;
        break;
    }
 return 0;
}